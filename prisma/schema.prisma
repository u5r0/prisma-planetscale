datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

// define a User table/collection
model User {
  id Int @id @default(autoincrement())
  firstName String
  lastName String
  age Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
